          <contentElements xsi:type="org.eclipse.epf.uma:Artifact" xmi:id="_XhzOoDC5EemRxcCHipfEmA" name="test_plan" guid="_XhzOoDC5EemRxcCHipfEmA" presentationName="Test Plan">
