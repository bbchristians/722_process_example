          <contentElements xsi:type="org.eclipse.epf.uma:Outcome" xmi:id="_DGo8kDC6EemRxcCHipfEmA" name="modified_unit_test_package" guid="_DGo8kDC6EemRxcCHipfEmA" presentationName="Modified Unit Test Package">
